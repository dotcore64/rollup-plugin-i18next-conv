{
  "extends": "airbnb-base",
  "rules": {
    "func-names": 0
  },
  "overrides": [
      {
          "files": ["test/{,**/}*.js"],
          "extends": ["plugin:mocha/recommended"],
          "plugins": ["mocha"],
          "env": {
              "mocha": true
          },
          "rules": {
            "mocha/no-mocha-arrows": 0
          }
      }
  ]
}
